@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<div class="row">
    <div class="small-12 columns">
        <h1 class="error">Error.</h1>
        <h2 class="error">An error occurred while processing your request.</h2>
        <label>@Model.ActionName</label><br />
        <label>@Model.ControllerName</label><br />
        <label>@Model.Exception.Message</label>
        <label>@Model.Exception.InnerException.Message</label>
    </div>    
</div>
    
